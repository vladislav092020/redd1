  <div class="modal non-active">
      <img src="./redaily_files/close.svg" alt="close" class="modal__close">
      <img src="./redaily_files/modal.jpg" alt="" class="modal__img">
      <div class="modal__content">
         <img src="./redaily_files/logo.svg" alt="logo" class="modal__logo">
         <p class="modal__text">Sign-up for</p>
         <p class="modal__discount">10% off</p>
         <img src="./redaily_files/border.svg" alt="border" class="modal__border">
         <form action="/" class="modal__form" method="post">
            <input type="email" class="modal__input" placeholder="Enter your email here">
            <button class="modal__button">GET 10% OFF</button>
         </form>
         <p class="modal__warning">By signing up you agree to Redailyâ€™s Privacy Policy</p>
      </div>
   </div>
 
 
 
 <!-- header -->
   <header class="nav _wrapper">
      <div class="nav__container _container">

         <!-- Burger-menu -->
         <button class="nav-toggle">
            <span class="bar-top"></span>
            <span class="bar-mid"></span>
            <span class="bar-bot"></span>
         </button>

         <!-- Logo -->
         <a href="/#main" class="nav__logo-link">
            <img src="./redaily_files/logo.svg" alt="redaily" class="nav__logo-img">
         </a>

         <!-- Navigation -->
         <div class="nav__burger">
            <ul class="nav__list">
              {%  for link in section.settings.menu.links %}
               <li class="nav__item"><a href="{{ link.url}}" class="nav__link">{{ link.title }}</a></li>
               {%  endfor %}
              
            </ul>
            <div class="nav__language-block">
               <a href="/" class="nav__language">English</a>
            </div>
         </div>

         <!-- Basket -->
         <div class="nav__basket-block">
